[core]
	editor = nvim
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	side-by-side = true
	line-numbers = true
[merge]
	conflictStyle = zdiff3
	tool = nvimdiff
[pull]
	rebase = false
[alias]
	br = branch
	cm = commit -m
	discard = reset HEAD --hard
	st = status
